    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--model_name_or_path",
                "microsoft/deberta-v3-base",
                "--input_format",
                "typed_entity_marker_punct",
                "--seed",
                "23",
                "--run_name",
                "deberta-v3-base",
                "--num_train_epochs",
                "5"
            ]
        }
    ]
}
